Configs: {
	Config: {
		DisplayName: Config

		Headers: {
			Config: Config
			ChatSettings: Chat Settings
			ChatFormat: Chat Format
			StatsViewer: Stats
			ShowButtons: Show Buttons
			Preview: Preview
		}

		Scrollbar: {
			Label: Scrollbar
			Tooltip: Shows a scrollbar when the number of messages exceed the visible limit.
		}

		ChatsVisible: {
			Label: Chats Visible
			Tooltip: Set the number of chat messages that can be visible at one time.
		}

		ModIcon: {
			Label: Mod Icon
			Tooltip: Shows which mod writes to chat.
		}

		PlayerIcon: {
			Label: Player Icon
			Tooltip: Shows who writes to chat.
		}

		PlayerColor: {
			Label: Player Color
			Tooltip: ""
		}

		TextEditor: {
			Label: Text Editor
			Tooltip:
				'''
				Improves the text editor with the following shortcuts:
				Ctrl+C: Copy
				Ctrl+V: Paste
				Ctrl+X: Cut
				Ctrl+A: Select all
				Arrows: Move cursor
				Shift+Arrows: Select characters
				Ctrl+Shift+Arrows: Select words
				'''
		}

		ShowStatsWhenHovering: {
			Label: Show Stats When Hovering
			Tooltip: Shows stats when hovering above a player or mod icon.
		}

		ShowUploadsWhenHovering: {
			Label: Show Uploads When Hovering
			Tooltip: Shows a bigger upload when hovering.
		}

		ShowStatsWhenBossIsAlive: {
			Label: Show Stats When Boss Is Alive
			Tooltip: Keep this off to disable stats hovering when a boss is alive.
		}

		StatsPrivacy: {
			Label: Stats Privacy
			Tooltip:
				'''
				Only you: Only you can view your stats.
				Team: Only players on your team can view your stats.
				Everyone: Everyone on the server can view your stats.
				'''
		}

		timestampSettings: {
			Label: Timestamp
			Tooltip: Shows when a chat was written.
		}

		TypingIndicators: {
			Label: Typing Indicators
			Tooltip: Shows when a player is currently writing a chat and shares when you are writing a chat.
		}

		viewmode: {
			Label: View Mode
			Tooltip:
				'''
				List View: Displays content vertically in a single column.
				Grid View: Displays content in a horizontal and vertical arrangement of cells.
				'''
		}

		Autocomplete: {
			Label: Autocomplete
			Tooltip:
				'''
				Opens an autocomplete window for the following prefixes:
				/ for commands
				: for emojis
				# for uploads
				@ for player mentions
				[c for colors
				[g for glyphs
				[i for items
				[m for mod icons
				[p for player icons
				'''
		}

