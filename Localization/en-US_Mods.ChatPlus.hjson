Commands: {
	TagsCommand: {
		Usage: /tags
		Description: Shows all tags available in Chat+.
		TagsHeader: "{0}Tags:{1}"
		Tag_1: Colors      [c
		Tag_2: Emojis      [e or :
		Tag_3: Glyphs      [g
		Tag_4: Items        [i
		Tag_5: Mod Icons  [m
		Tag_6: Players      [p or @
		Tag_7: Uploads     [u or #
	}

	ChatClearCommand: {
		Usage: /clear
		Description: Clears all chat messages.
		Cleared: Chat cleared!
	}

	ModsCommand: {
		Description: List the names of all the mods
		Usage: List the names of all the mods
	}
}

